<%@ Control Language="C#" AutoEventWireup="true" CodeFile="IngdntListWebUserControl.ascx.cs" Inherits="IngdntListWebUserControl" %>
<%@ Register Src="IngdntListWebUserControl.ascx" TagPrefix="uc1" TagName="IngdntList" %>
<p>
    <asp:Label ID="lblInName" runat="server" Text="Name"></asp:Label>
    <asp:TextBox ID="txtInName" runat="server"></asp:TextBox><asp:RequiredFieldValidator ID="InNameValid" runat="server" ErrorMessage="Name required"></asp:RequiredFieldValidator>
    <asp:Label ID="lblInQuantity" runat="server" Text="Name"></asp:Label>
    <asp:TextBox ID="txtInquantity" runat="server"></asp:TextBox><asp:RegularExpressionValidator ID="quantityValid" runat="server" ErrorMessage="Quantity should be a number" ValidationExpression="^[0-9]*$"></asp:RegularExpressionValidator>
</p>